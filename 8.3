#include <iostream>
#include <string>
#include <queue>
#include <Windows.h>
using namespace std;

class Client {
	string name;
	string car;
public:
	Client(string& name, string& car) : name(name), car(car) {};

	void getData();	
};

int main() {
	queue<Client> q;	

	for (int i = 0; i < 3; i++) {
		string name, car;
		cout << "Client " << i+1 << endl;
		cout << "Enter name: ";
		cin >> name;
		cout << "Enter car model: ";
		cin >> car;
		q.push(Client(name, car));	
	}

	cout << "Queue" << endl;
	for (int i = 1; !q.empty(); i++) {
		cout << i;
		q.front().getData();	
		q.pop();
	}

	system("pause");
	return 0;
}

